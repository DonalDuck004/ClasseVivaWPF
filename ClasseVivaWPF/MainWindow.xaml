        xmlns:local="clr-namespace:ClasseVivaWPF" KeyDown="window_KeyDown"
        mc:Ignorable="d" Name="window" Loaded="window_Loaded" Closing="window_Closing"
        Title="MainWindow" MinHeight="562.5" MinWidth="1000">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="20"></RowDefinition>
