        <ControlTemplate x:Key="CVMainMenuIcon">
            <ControlTemplate.Resources>
                <converters:ColorIfConverter x:Key="SelectedConverter" FalseColor="Gray" TrueColor="{StaticResource ResourceKey=CV_RED_COLOR}"></converters:ColorIfConverter>
                <converters:ColorIfConverter x:Key="T2WConverter" FalseColor="{StaticResource ResourceKey=CV_OPAQUE_WHITE_COLOR}" TrueColor="{StaticResource ResourceKey=CV_RED_COLOR}"></converters:ColorIfConverter>
            </ControlTemplate.Resources>
            <StackPanel x:Name="wp" Cursor="Hand">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="4"></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition Height="32"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Rectangle Grid.Row="0" 
                               Fill="{Binding IsSelected, 
                                              RelativeSource={RelativeSource TemplatedParent},
                                              Converter={StaticResource ResourceKey=T2WConverter}}"
                               Name="Top"></Rectangle>
                    <StackPanel Grid.Row="1" Background="Transparent" Width="64" Height="32">
                        <ContentControl x:Name="img" 
                                        Background="{Binding IsSelected, 
                                                             RelativeSource={RelativeSource TemplatedParent},
                                                             Converter={StaticResource ResourceKey=SelectedConverter}}"
                                        Foreground="{Binding IsSelected, 
                                                             RelativeSource={RelativeSource TemplatedParent},
                                                             Converter={StaticResource ResourceKey=SelectedConverter}}"
                                        Template="{Binding Icon, RelativeSource={RelativeSource TemplatedParent}}"
                                        HorizontalAlignment="Center"/>
                    </StackPanel>
                    <Label Grid.Row="2" x:Name="desc"
                           Content="{Binding IconValue, 
                                             RelativeSource={RelativeSource TemplatedParent}}"
                           Foreground="{Binding IsSelected, 
                                                RelativeSource={RelativeSource TemplatedParent},
                                                Converter={StaticResource ResourceKey=SelectedConverter}}" 
                           HorizontalAlignment="Center"
                           VerticalAlignment="Top" FontSize="11">
                        <Label.Resources>
                            <Style TargetType="Label">
                                <Style.Triggers>
                                    <DataTrigger Binding="{Binding IsSelected, RelativeSource={RelativeSource TemplatedParent}}" Value="True">
                                        <DataTrigger.EnterActions>
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <DoubleAnimation Storyboard.TargetProperty="FontSize" From="10" To="15" Duration="0:0:0.15"></DoubleAnimation>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </DataTrigger.EnterActions>
                                        <DataTrigger.ExitActions>
                                            <BeginStoryboard>
                                                <Storyboard>
                                                    <DoubleAnimation Storyboard.TargetProperty="FontSize" To="10" Duration="0:0:0.15"></DoubleAnimation>
                                                </Storyboard>
                                            </BeginStoryboard>
                                        </DataTrigger.ExitActions>
                                    </DataTrigger>
                                </Style.Triggers>
                            </Style>
                        </Label.Resources>

                    </Label>
                </Grid>
            </StackPanel>
        </ControlTemplate>

